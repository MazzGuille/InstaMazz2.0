
@{
    ViewBag.Title = "FeedView";
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Feed = ViewBag.Feed;
}
<head>
    <link href="~/Content/CSS/Perfil.css" rel="stylesheet" />
    
</head>

@foreach (var item in Feed)
{ 
    <div class="container-fluid" style="display: flex; flex-direction: column; align-items: center; justify-content: left; gap:2px;">
        @*<img @item.UrlImg style="width: 200px; height: 200px;" />*@
        <img class="img-responsive imagen-feed" src="@item.UrlImg" />
        <div style="width:50vw; flex; flex-direction: column; align-items:flex-start; justify-content:left; gap: 3px;">
            <h3 class="feed-UsNam">@item.UserName</h3>
            <p class="feed-desc">@item.Descripcion</p>
        </div>
        <br />
    </div>
}

